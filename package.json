{
  "name": "react-webpack-babel",
  "version": "0.0.3",
  "description": "React Webpack Babel Starter Kit",
  "main": "''",
  "scripts": {
    "build": "webpack --config ./webpack/webpack.production.config.js --progress --profile --colors",
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --progress --profile --colors",
    "lint": "eslint --ext jsx --ext js ./src || exit 0",
    "build-css-rtl": "node-sass --source-map true --include-path public sass/base_rtl.scss   public/css/style-rtl.css autoprefixer",
    "build-css-ltr": "node-sass --source-map  true --include-path sass sass/base_ltr.scss   public/css/style-ltr.css autoprefixer",
    "build-css": "npm run build-css-rtl && npm run build-css-ltr",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "swagger": "./bin/webpack.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alicoding/react-webpack-babel"
  },
  "author": "Ali Al Dallal",
  "license": "MIT",
  "homepage": "https://github.com/alicoding/react-webpack-babel#readme",
